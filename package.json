{
  "name": "everreal",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "types": "tsc --noEmit",
    "test": "npm start & cypress run",
    "pretest": "npm run lint && npm run types",
    "print-env": "print-env",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "e2e:record:edge": "cypress run --record --browser edge",
    "e2e:record:parallel": "cypress run --record --parallel",
    "test:ci": "run-p --race start:ci e2e",
    "test:ci:chrome": "run-p --race start:ci e2e:chrome",
    "test:ci:record": "run-p --race start:ci e2e:record",
    "test:ci:record:windows": "run-p --race start:ci:windows e2e:record",
    "test:ci:record:windows:edge": "run-p --race start:ci:windows e2e:record:edge",
    "test:ci:record:parallel": "run-p --race start:ci e2e:record:parallel",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record --key 4a8cd431-6953-4bf6-bb55-5682bed8a030",
    "cy:open": "cypress open",
    "lint": "tslint './cypress/**/*.ts?(x)'",
    "lint-fix": "tslint './cypress/**/*.ts?(x)' --fix",
    "pretty-fix": "prettier './cypress/**/*.ts?(x)' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@bahmutov/print-env": "^1.2.1",
    "@everreal/er-common-helpers": "^1.100.5",
    "@types/config": "0.0.36",
    "@types/uuid": "^7.0.2",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cypress-downloadfile": "^1.1.5",
    "cypress-file-upload": "^3.5.3",
    "cypress-xpath": "^1.4.0",
    "emailjs-imap-client": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.8",
    "cypress": "^4.7.0",
    "cypress-cucumber-preprocessor": "^2.0.1",
    "cypress-multi-reporters": "^1.2.4",
    "husky": "^4.2.3",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0"
  }
}
